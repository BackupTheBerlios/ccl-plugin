abstract class $ANTLR_EXCEPTION < $STR is
   str : STR;
end;

class ANTLR_EXCEPTION < $ANTLR_EXCEPTION is
   attr str : STR;

   message : STR is
      return str;
   end;

   create ( s : STR ) : SAME is
      res ::= new;
      res.str := s;
      return res;
   end;
   
end;

